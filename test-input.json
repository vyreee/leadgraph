{
  "seedType": "keyword",
  "keywords": ["plumber", "plumbing service"],
  "locations": ["Denver, CO"],
  "useApis": true,
  "sources": ["googleMaps", "yelp"],
  "maxResultsPerLocation": 15,
  "maxTotalResults": 30,
  "filters": {
    "minRating": 4.0,
    "minReviews": 25,
    "requireWebsite": false,
    "excludeFranchises": false,
    "excludeKeywords": [],
    "includeKeywords": []
  },
  "enrichment": {
    "crawlWebsite": true,
    "maxWebsitePages": 5,
    "emailExtraction": "standard",
    "phoneExtraction": true,
    "collectTechSignals": true
  },
  "dedupe": {
    "enabled": true,
    "strategy": "balanced"
  },
  "scoring": {
    "enabled": true,
    "weightsPreset": "localService"
  },
  "ai": {
    "enabled": true,
    "provider": "openai",
    "model": "gpt-4o-mini",
    "yourCompany": {
      "name": "Digital Growth Partners",
      "services": "website design, SEO, and lead generation",
      "targetAudience": "local service businesses"
    }
  },
  "exports": {
    "outputFormat": "dataset",
    "deltaMode": false,
    "webhookUrl": "",
    "webhookMode": "off"
  },
  "runMode": {
    "stealth": true,
    "maxConcurrency": 2,
    "maxRetries": 3,
    "proxyEnabled": false
  },
  "debug": {
    "saveHtmlSnapshots": false,
    "logLevel": "INFO"
  }
}
